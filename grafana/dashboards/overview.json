          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toStartOfMinute(ingest_ts) AS time,\n  count() / 60 AS eps\nFROM analytics.fact_event_dlq\nWHERE $__timeFilter(ingest_ts)\nGROUP BY time\nORDER BY time ASC",
          "refId": "A"
        }

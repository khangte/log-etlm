          "rawQuery": true,
          "rawSql": "WITH\n  (SELECT min(date) FROM analytics.dim_date) AS min_date,\n  (SELECT max(date) FROM analytics.dim_date) AS max_date,\n  dateDiff('day', min_date, max_date) + 1 AS expected,\n  (SELECT count() FROM analytics.dim_date) AS actual\nSELECT\n  min_date,\n  max_date,\n  expected,\n  actual,\n  expected - actual AS missing_days",

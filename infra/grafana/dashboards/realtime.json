{
  "_comment": "Grafana 로그 파이프라인 Realtime (10s raw query 기반)",
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.4.2"
    },
    {
      "type": "datasource",
      "id": "grafana-clickhouse-datasource",
      "name": "ClickHouse",
      "version": "4.x"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "1"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "1"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": {
          "limit": 100,
          "matchAny": false,
          "tags": [],
          "type": "dashboard"
        },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "fact_event 원본을 10초 버킷으로 집계한 EPS입니다. (실시간 모니터링용)",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "EPS",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "eps"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "id": 1,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket,\n  countMerge(total_state) / 10 AS eps\nFROM analytics.fact_event_agg_10s\nWHERE $__timeFilter(bucket)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "로그 EPS (ingest_ts 기준) (10s)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "fact_event 원본 기준 10초 버킷 에러율(5xx)입니다.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "error %",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "id": 2,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket,\n  if(total = 0, 0, errors / total) * 100 AS error_rate\nFROM (\n  SELECT\n    bucket,\n    countMerge(total_state) AS total,\n    countMerge(errors_state) AS errors\n  FROM analytics.fact_event_agg_10s\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n)\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "에러율 (ingest 기준) (10s)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "event_ts ~ stored_ts 기준 E2E p95 (10초 버킷).",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "p95 (ms)",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "id": 3,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket AS time,\n  quantileTDigestMerge(0.95)(e2e_state) AS p95_ms\nFROM analytics.fact_event_latency_10s\nWHERE $__timeFilter(bucket)\nGROUP BY bucket\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "End-to-End 지연 p95 (10s)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "created_ts 기준 단계별 지연 p95 (10초 버킷).",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "p95 (ms)",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "id": 4,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket AS time,\n  quantileTDigestMerge(0.95)(producer_to_kafka_state) AS producer_to_kafka_p95_ms,\n  quantileTDigestMerge(0.95)(kafka_to_spark_ingest_state) AS kafka_to_spark_ingest_p95_ms,\n  quantileTDigestMerge(0.95)(spark_processing_state) AS spark_processing_p95_ms,\n  quantileTDigestMerge(0.95)(spark_to_stored_state) AS spark_to_stored_p95_ms,\n  quantileTDigestMerge(0.95)(e2e_state) AS e2e_p95_ms\nFROM analytics.fact_event_latency_stage_10s\nWHERE $__timeFilter(bucket)\nGROUP BY bucket\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "과정별 지연 p95 (10s)",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
